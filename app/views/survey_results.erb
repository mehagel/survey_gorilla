<h2>Results for "<%= @survey.name %>"</h2>

<% @survey.questions.each do |question| %>

  <h4><%= question.content %></h4>
    <ul>
      <% question.choices.each do |choice| %>
        <li>
          <%= choice.content %>: <%= choice.votes.length %> votes
        </li>
      <% end %>
    </ul>

<% end %>